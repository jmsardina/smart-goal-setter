<tr id="activity_<%= activity.id %>">
  <%= form_for([goal, activity]) do |f| %>
    <td><%= f.check_box :status, class: "toggle" %></td>
  <% end %>
  <td><%= activity.description %></td>
  <td><%= activity.barrier %></td>
  <td><%= activity.facilitator %></td>
  <td><%= activity.frequency %></td>
  <td><%= activity.period %></td>
  <td><%= activity.activity_points %></td>
  <td><%= activity.remaining_for_period %></td>
  <td><%= activity.upcoming_deadline %></td>
  <td><button class="destroy"></button></td>
</tr>